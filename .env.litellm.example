# Shared internal key used by deepbot.
# Must match OPENAI_API_KEY in .env.deepbot.
LITELLM_MASTER_KEY=change-this-litellm-key

# Provider keys
OPENAI_API_KEY=your_openai_api_key

# Optional: enable GLM route in config/litellm.yaml
GLM_API_KEY=
GLM_API_BASE=https://open.bigmodel.cn/api/paas/v4
